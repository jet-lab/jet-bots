{
    "include": [],
    "compilerOptions": {
        "target": "ESNext",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "composite": true,
        "declaration": true,
        "declarationMap": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "noEmitOnError": true,
        "resolveJsonModule": true,
        "sourceMap": true,
        "strict": true,
        "stripInternal": true
    }
}
